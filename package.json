{
  "name": "R6RC",
  "version": "0.4.2",
  "main": "main.js",
  "repository": "https://github.com/austinleath/r6rc",
  "license": "MIT",
  "description": "A Minimalistic Rank Calculator For Rainbow Six: Siege",
  "author": "Austin Leath",
  "scripts": {
    "start": "electron .",
    "testwin": "build --win",
    "testmac": "build --mac",
    "testlin": "build --linux",
    "publish": "build -p always",
    "updep": "npm install electron@latest electron-builder@latest electron-packager@latest electron-log@latest electron-updater@latest"
  },
  "devDependencies": {
    "electron": "^3.0.2",
    "electron-builder": "^20.28.4",
    "electron-packager": "^12.1.2"
  },
  "dependencies": {
    "electron-log": "^2.2.17",
    "electron-updater": "^3.1.2",
    "universal-analytics": "^0.4.17"
  },
  "build": {
    "appId": "com.austinleath.r6rc",
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "zip",
        "dmg"
      ]
    }
  }
}
