{
  "name": "R6RC",
  "version": "0.4.3",
  "main": "main.js",
  "repository": "https://github.com/austinleath/r6rc",
  "license": "MIT",
  "description": "A Minimalistic Rank Calculator For Rainbow Six: Siege",
  "author": "Austin Leath",
  "scripts": {
    "start": "electron .",
    "testwin": "build --win",
    "testmac": "build --mac",
    "testlin": "build --linux",
    "publish": "build -p always",
    "updep": "npm install universal-analytics@latest electron@latest electron-builder@latest electron-packager@latest electron-log@latest electron-updater@latest"
  },
  "devDependencies": {
    "electron": "^3.0.7",
    "electron-builder": "^20.29.0",
    "electron-packager": "^12.2.0"
  },
  "dependencies": {
    "electron-log": "^2.2.17",
    "electron-updater": "^3.1.6",
    "universal-analytics": "^0.4.20"
  },
  "build": {
    "appId": "com.austinleath.r6rc",
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "zip",
        "dmg"
      ]
    }
  }
}
